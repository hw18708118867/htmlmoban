/** 导航菜单 **/
#navbar {
	height:41px;
	margin:0px auto;
	position:relative;
}
#navbar ul {
	list-style:none;
}
#navbar li {
	float:left;
	position:relative;
	background:url(../images/li.png) no-repeat right 50%;
	padding-right:2px;
	width:110px;
	text-align:center;
}
#navbar li.end {
	background:none;
}
#navbar li.current-menu-item a {
	color:#fff;
	text-decoration:none;
}
#navbar ul.sub-menu li.current-menu-item a {
	background:none;
}
#navbar li a {
	/*width:54px;
	padding:0 19px 0 18px;*/
	height:41px;
	line-height:41px;
	display:block;
	color:#f8f8f8;
	font-weight:bold;
	font-size:14px;
	white-space:nowrap;
	
}
#navbar li a:hover {
	color:#ffffff;
	text-decoration:none;
	background:#0684E1;
}
#navbar .sub-menu {
	position:absolute;
	background-color:#0684e1;
	top:41px;
	width:122px;
	display:none;
	z-index:9999;
}
#navbar .sub-menu li {
	float:none;
	background:none;
	border-bottom:1px solid #0684E1;
}
#navbar .sub-menu li a {
	float:none;
	width:107px;
	height:33px;
	padding:0 0 0 25px;
	line-height:33px;
	font-size:12px;
	font-weight:normal;
	display:block;
	color:#fff;
}
#navbar .sub-menu li a:hover {
	color:#fff;
	text-decoration:none;
	background:none;
}
#navbar li.current-menu-item .sub-menu li a {
	color:#fff;
	text-decoration:none;
	background:none;
}
#navbar .sub-menu .sub-menu {
	left:122px;
	top:0px;
	-left:122px;
	-top:0px;
}
#navbar li .sub-menu li a.sub {
	background:url(../images/misc_26.png) no-repeat right 50%;
}
.hot-ico {
	z-index:999;
	display:block;
	height:19px;
	line-height:12px;
	position:absolute;
	right:310px;
	top:-7px;
	width:22px;
	background:url(../images/hot-ico.gif) no-repeat scroll 0 0 transparent;
}
.new-ico {
	z-index:111;
	display:block;
	height:12px;
	line-height:12px;
	position:absolute;
	right:120px;
	top:0px;
	width:22px;
	background:url(../images/new-ico.gif) no-repeat scroll 0 0 transparent;
}
